{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
        "os": "#ACB0BE",
        "closer": "p:os",
        "pink": "#F5BDE6",
        "lavender": "#B7BDF8",
        "blue": "#8AADF4",
        "green": "#A6DA95",
        "yellow": "#EED49F",
        "red": "#ED8796"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "p:os",
          "style": "plain",
          "template": "{{.Icon}} ",
          "type": "os"
        },
        {
          "foreground": "p:pink",
          "properties": {
            "folder_icon": "..\ue5fe..",
            "home_icon": "~",
            "style": "agnoster_short"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "p:lavender",
          "properties": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "template": "{{ .HEAD }} ",
          "style": "plain",
          "type": "git"
        },
        {
          "style": "plain",
          "foreground": "p:closer",
          "template": "\uf105",
          "type": "text"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "p:yellow",
          "style": "plain",
          "template": "{{ if .Error }}\uf00d {{ .Code }}{{ else }}\uf00c{{ end }} ",
          "type": "status"
        },
        {
          "foreground": "p:lavender",
          "style": "plain",
          "template": "{{ if .Full }}\ue73d {{ .Full }} {{ end }}",
          "type": "php"
        },
        {
          "foreground": "p:blue",
          "style": "plain",
          "template": "{{ if .Full }}\ue627 {{ .Full }} {{ end }}",
          "type": "go"
        },
        {
          "foreground": "p:green",
          "style": "plain",
          "template": "{{ if .Full }}\ue718 {{ .Full }} {{ end }}",
          "type": "node"
        },
        {
          "foreground": "p:blue",
          "style": "plain",
          "template": "{{ if .Context }}\ue81d {{ .Context }} {{ end }}",
          "type": "kubectl"
        },
        {
          "foreground": "p:pink",
          "style": "plain",
          "template": "\uf073 {{ .CurrentDate | date \"Jan 2\" }} \uf017 {{ .CurrentDate | date \"15:04\" }} ",
          "type": "time"
        }
      ],
      "type": "rprompt"
    }
  ],
  "final_space": true,
  "version": 3
}